                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                    android:id="@+id/tvWorngAnalysis"
                    android:layout_width="wrap_content"
                    android:layout_height="53dp"
                    android:gravity="center_vertical"
                    android:textSize="18sp"
                    android:layout_marginStart="10dp"
                    android:fontFamily="sans-serif"
                    android:text="오답현황"
                    android:textColor="@color/black0"
                    android:letterSpacing="0.05"
                    app:layout_constraintStart_toStartOf="@+id/v1WorngNote"
                    app:layout_constraintTop_toBottomOf="@+id/wrong_analysis_global_title"/>

            <View
                    android:id="@+id/v1WorngNote"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/gray2"
                    app:layout_constraintTop_toBottomOf="@+id/tvWorngAnalysis"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="@dimen/activity_margin20"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="@dimen/activity_margin20"/>


            <com.smu.sangmyung.quiz.worng.LineGraphView
                    android:id="@+id/LineGraphView"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:layout_marginStart="@dimen/SIZE_5"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="@dimen/SIZE_5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/v1WorngNote"
                    app:lineColor="#707070"
                    app:lineThickness="2dp"
                    app:pointSize="8dp"
                    app:textColor="#858585"
                    app:textGap="10dp"
                    app:textSize="11sp" app:layout_constraintHorizontal_bias="0.0"/>
            <LinearLayout
                    android:weightSum="7"
                    android:id="@+id/tvLineGraphView"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    app:layout_constraintStart_toStartOf="@id/LineGraphView"
                    app:layout_constraintEnd_toEndOf="@id/LineGraphView"
                    app:layout_constraintTop_toBottomOf="@+id/LineGraphView">

                <TextView
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="normal"
                        android:textSize="10sp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="@string/algorithm_h"
                />
                <TextView
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="normal"
                        android:textSize="11sp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="@string/database_h"
                />
                <TextView
