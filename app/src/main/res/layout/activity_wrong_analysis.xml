                        app:layout_constraintTop_toTopOf="@+id/circleGraph_Algo"
                        app:layout_constraintBottom_toTopOf="@+id/tvAlgotext"
                        app:layout_constraintStart_toEndOf="@+id/circleGraph_Algo"
                        android:layout_marginStart="8dp"
                />
                <TextView
                        android:text="75%의 정답률
    총 푼 문제의 수 : 220"
